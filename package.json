{
  "name": "react-tdd-redux-practice",
  "version": "0.1.0",
  "description": "This is my practice of TDD of react with react testing library and cypress.",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "axios": "^0.21.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "redux-thunk": "^2.3.0",
    "uid": "^2.0.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testPathIgnorePatterns=src/__tests__/e2e",
    "eject": "react-scripts ej,ect",
    "cypress:open": "cypress open --project ./src/__tests__/e2e",
    "cypress:run": "cypress run --project ./src/__tests__/e2e --record --key 22f03873-d7be-4dfb-a1b4-c2b8e3e6dda1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.6.0",
    "bootstrap": "^4.5.3",
    "cypress": "^6.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.4",
    "redux-mock-store-await-actions": "^2.1.0"
  }
}
